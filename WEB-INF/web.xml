<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<servlet>
		<servlet-name>addfield</servlet-name>
		<servlet-class>servlet.AddField</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>addfield</servlet-name>
		<url-pattern>/addfield</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>timepass</servlet-name>
		<servlet-class>servlet.TimePass</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>timepass</servlet-name>
		<url-pattern>/timepass</url-pattern>
	</servlet-mapping>

</web-app>
